<h1>Create new product</h1>
<!--form builder -->
  <%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  
  <%= f.submit "Create New Product"%>
<%end%>